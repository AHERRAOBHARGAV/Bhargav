#include <iostream>
using namespace std;

int main() {
    int m,n,i,j;
    int numbers[3][3];

    cout << "Enter 9 numbers: " << endl;

  
    for (int i=0;i<3;i++)
    {
        for (int j=0;j<3;j++)
        {
            cin >> numbers[i][j];
        }
    }

    cout << "enter the numbers : " << endl;

  
 
    for (int i=0;i<3;i++){
       for (int j=0;j<3;j++)
       cout << numbers[i][j] << " ";
       cout << endl;
}
bool check(int a[n][m]){
   for (int i=0;i<3;i++)
      for (int j=0;j<3;j++)
         if (a[i][j] = 0)
            return false;
   return true;
}

      cout << "its a lower triangular matrix";
   else
      cout << "its not a lower triangular matrix";
   return 0;
}

    return 0;
